<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="AppCompat" parent="AppBaseTheme">
        <item name="android:fitsSystemWindows">true</item>
        ></style>

    <style name="blue_tooth_dialog_select">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/navy</item>
    </style>

    <style name="dialog_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@drawable/dialog_background</item>
    </style>

    <style name="homepage_item_textview1">
        <item name="android:textSize">19sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="homepage_item_textview2">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/darkgray</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="action_item_textview">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/dimgray</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">2dp</item>
    </style>

    <style name="action_item_textview2">
        <item name="android:textSize">29sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="sleepText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#E6DFE6</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="sleepTextDown">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">#E6DFE6</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="sleepData">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">22dp</item>
    </style>

    <style name="sleepDataDown">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">3dp</item>
    </style>

    <style name="act_action_item_textview">
        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">5dp</item>
    </style>
    <style name="act_action_item_textview2">
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">@color/orange</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">5dp</item>
    </style>
  <style name="editstyle">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#6a6a6a</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">70dp</item>
    </style>
    
    <style name="showstyle">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">#858585</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">13dp</item>
    </style>
    
    <style name="anotherShowstyle">
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">#5A5A5A</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
		<item name="android:layout_marginRight">6dp</item>
    </style>
    
    <style name="wheelViewStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="itemNumber">3</item>
        <item name="lineColor">#ff6600</item>
        <item name="bottomLineColor">#ffdadada</item>
        <item name="lineHeight">3dp</item>
        <item name="firstLineAndSecondLineSpace">10dp</item>
        <item name="maskHight">32dp</item>
        <item name="noEmpty">true</item>
        <item name="normalTextColor">#99999999</item>
        <item name="normalTextSize">14sp</item>
        <item name="selectedTextColor">#222222</item>
        <item name="selectedTextSize">22sp</item>
        <item name="unitHight">50dp</item>
    </style>
    
     <style name="dialog" parent="android:style/Theme.Dialog">
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    
</resources>